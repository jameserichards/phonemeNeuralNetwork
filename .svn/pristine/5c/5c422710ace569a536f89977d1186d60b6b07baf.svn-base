package speech;

import javax.swing.JApplet;


public class MainApplet extends JApplet {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Override
	public void init() {
		MainApp app = new MainApp(true);
		
		try {
			app.start();
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}			
	}

}
